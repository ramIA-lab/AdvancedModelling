                        barris = c("Cente", "Sanfeliu", "Sant Josep", "Collblanc", 
                                   "La Torrassa", "Santa EulÃ lia", "La Florida", 
                                   "Les Planes", "Can Serra", "Publilla Cases", 
                                   "El Gornal", "Bellvitge"))

